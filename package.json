{
  "name": "kakapo-app",
  "version": "1.3.0",
  "description": "A background noise app",
  "main": "browser.js",
  "repository": "git+https://github.com/bluedaniel/Kakapo-app.git",
  "author": "Daniel Levitt @supercerebral",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bluedaniel/Kakapo-app/issues"
  },
  "homepage": "https://github.com/bluedaniel/Kakapo-app#readme",
  "scripts": {
    "build": "babel-node tools/run build",
    "clean": "babel-node tools/run clean",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "installer-mac": "babel-node tools/run installer-mac",
    "installer-win": "babel-node tools/run installer-win",
    "lint": "stylelint \"app/scripts/**/*.css\" && eslint '__tests__/**/*.js' 'app/scripts/**/*.js'",
    "pkg": "babel-node tools/run pkg",
    "start": "babel-node tools/run start",
    "styles:format": "stylefmt -r \"app/**/*.css\"",
    "test": "npm run lint && cross-env NODE_ENV=test jest",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./jest/setup.js",
      "jest-localstorage-mock"
    ],
    "testMatch": [
      "**/__tests__/**/*.spec.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^kakapoBridge$": "<rootDir>/app/scripts/bridge/web/index.js"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "app/scripts/**/*.{js,jsx}",
      "!app/scripts/bridge/desktop/**",
      "!app/scripts/*.js"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "clipboard": "^1.7.1",
    "color": "^3.0.0",
    "fs-extra": "^5.0.0",
    "hint.css": "^2.3.2",
    "history": "^4.7.2",
    "howler": "^2.0.9",
    "intl": "^1.2.4",
    "kakapo-assets": "^5.18.0",
    "lost": "^8.0.0",
    "query-string": "^5.1.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.8",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-transition-group": "^2.2.1",
    "recompose": "^0.26.0",
    "redbox-react": "^1.5.0",
    "redux": "^3.4.0",
    "redux-saga": "^0.16.0",
    "reduxsauce": "^0.7.0",
    "request": "^2.83.0",
    "semver": "^5.5.0",
    "shortid": "^2.2.6",
    "whatwg-fetch": "^2.0.3",
    "ytdl-core": "^0.20.2"
  },
  "devDependencies": {
    "archiver": "^2.1.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "compression-webpack-plugin": "^1.1.8",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "cssnano": "^3.7.3",
    "electron": "^1.8.2",
    "electron-packager": "^11.0.1",
    "electron-winstaller": "^2.6.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "exports-loader": "^0.7.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "husky": "^0.14.3",
    "imagemin": "^5.2.2",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-pngquant": "^5.0.1",
    "imports-loader": "^0.8.0",
    "jest": "^22.4.2",
    "jest-fetch-mock": "^1.4.2",
    "jest-localstorage-mock": "^2.2.0",
    "lint-staged": "^7.0.0",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "ora": "^2.0.0",
    "postcss": "^6.0.19",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.0",
    "postcss-nested": "^3.0.0",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.3.0",
    "prettier": "^1.10.2",
    "raf": "^3.4.0",
    "rcedit": "^1.0.0",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.5.1",
    "style-loader": "^0.20.2",
    "stylefmt": "^6.0.0",
    "stylelint": "^9.1.1",
    "stylelint-config-standard": "^18.1.0",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.21.0",
    "yargs": "^11.0.0"
  }
}

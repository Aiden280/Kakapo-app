{
  "presets": ["node5", "react", "stage-0"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./app/scripts"],
        "alias": {
          "aws-custom-build": "./app/vendor/aws-sdk-2.2.31.min"
        }
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        [
          "module-resolver",
          {
            "root": ["./app/scripts"],
            "alias": {
              "aws-custom-build": "./app/vendor/aws-sdk-2.2.31.min",
              "kakapoBridge": "./app/scripts/bridge/web/index.js"
            }
          }
        ]
      ]
    }
  }
}
